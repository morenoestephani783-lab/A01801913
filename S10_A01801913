segundos = int(input("Ingresa la cantidad de segundos: "))
horas = segundos // 3600
segundos = segundos % 3600
minutos = segundos // 60
segundos = segundos % 60
print("Eso equivale a "horas","horas","minutos","minutos","segundos,"segundos")
